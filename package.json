{
  "name": "zeta-cms-ui",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build:app": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "ts-standard ./src",
    "lint:fix": "ts-standard --fix ./src",
    "scaffold": "plop",
    "component:create": "plop component",
    "page:create": "plop page",
    "compile": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
    "build:clean": "rm -rf ./dist",
    "build:code": "webpack --config webpack.config.js",
    "build": "npm run build:clean && npm run build:code",
    "storybook": "storybook dev"
  },
  "exports": {
    ".": {
      "types": "./build/types/lib.d.ts",
      "require": "./build/cjs/lib.js",
      "import": "./build/esm/lib.js",
      "default": "./build/esm/lib.js"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:react-hooks/recommended"
    ]
  },
  "overrides": {
    "typescript": "^5.2.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.7",
    "@mui/joy": "^5.0.0-beta.9",
    "@mui/material": "^5.14.13",
    "@mui/x-data-grid": "^6.15.0",
    "@mui/x-date-pickers": "^6.18.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@storybook/react": "^7.4.0",
    "@tabler/icons-react": "^2.40.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.169",
    "@types/node": "^20.5.9",
    "@types/parse-link-header": "^2.0.3",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "dayjs": "^1.11.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "lodash": "^4.17.21",
    "notistack": "^3.0.1",
    "parse-link-header": "^2.0.0",
    "plop": "^3.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-icons": "^4.8.0",
    "react-player": "^2.13.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1",
    "redux-persist": "^6.0.0",
    "storybook": "^7.4.0",
    "ts-loader": "^9.5.0",
    "ts-standard": "^12.0.2",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.2.2",
    "web-vitals": "^2.1.4",
    "webpack-cli": "^5.1.4",
    "zeta-react-forms": "git+ssh://git@github.comcast.com/Corp-Comm/zeta-react-forms"
