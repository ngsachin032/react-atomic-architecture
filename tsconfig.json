{
  "compilerOptions": {
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": false,
    "strictNullChecks": true,
    "baseUrl": "src/",
    "paths": {
      "@/*": ["src/*"]
    },
    "lib": [
      "decorators",
      "dom",
      "es5",
      "scripthost",
      "es2015.promise"
    ]
  },
  "include": [
    "src"
  ]
}